    <section class="adviceSections comments">


      <%allData.forEach(val=>{%>
      <div class="addedComment">
        <div class="profileContainer theCommentDivs">
          <img style="width: 5em;" class="profileImage"
            src="https://www.voanews.com/themes/custom/voa/images/Author__Placeholder.png" alt="profilePlacholder">
          <p class="name"> <%=val.name%></p>
          <p class="country"> <%=val.country%></p>
        </div>
        <p class="comment theCommentDivs"> <%=val.comment%></p>
        <div class="buttonContainer theCommentDivs">
          <a class="edit" href="/update/<%=val.id%>"><button class="editBtn" style='font-size:24px'><i
                class='fas fa-edit'></i></button></a>
          <form action="/delete/<%=val.id%>?_method=delete" method="post">
            <button type="submit" class="deleteBtn edit" style='font-size:24px'><i
                class='fas fa-trash-alt'></i></button>
          </form>
        </div>
      </div>
      <%})%>
    </section>
    <a href="/form"><button class="addBtn">Add Comment</button></a>
